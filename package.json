{
  "type": "module",
  "engines": {
    "node": ">= 14.0.0"
  },
  "name": "pnp-react-hooks",
  "version": "1.2.0",
  "description": "PnPJS React hooks library.",
  "main": "index.js",
  "scripts": {
    "build": "node ./scripts/build.js",
    "build-debug": "node ./scripts/build.js -c Debug",
    "pack-local": "node ./scripts/build.js -c Release && node ./scripts/omitPackageJson.js --outDir .temp/bin/release && npm pack ./.temp/bin/release --pack-destination ./.temp/bin/",
    "pack": "node ./scripts/build.js -c Release && node ./scripts/omitPackageJson.js --outDir .temp/bin/release",
    "clean": "node ./scripts/clean.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js src/**/*",
    "test": "jest"
  },
  "keywords": [
    "pnp",
    "sharepoint",
    "react",
    "sp",
    "hooks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SuperioOne/pnp-react-hooks"
  },
  "homepage": "https://superioone.github.io/pnp-react-hooks",
  "author": "Timur Olur",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-transform-modules-commonjs": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pnp/nodejs": "^3.6.0",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/args": "^5.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.2.4",
    "@types/jsdom": "^21.1.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "args": "^5.0.3",
    "babel-jest": "^29.3.1",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.1.0",
    "jest": "^29.3.1",
    "jsdom": "^21.1.0",
    "node-fetch": "^3.3.0",
    "react-dom": "^17.0.2",
    "rollup": "^3.8.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-visualizer": "^5.8.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
