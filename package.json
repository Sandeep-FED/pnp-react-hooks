{
  "type":"module",
  "engines": {
    "node": ">= 14.0.0"
  },
  "name": "pnp-react-hooks",
  "version": "0.0.0",
  "description": "PnPJS React hooks library.",
  "main": "index.js",
  "scripts": {
    "build": "node ./scripts/build.js -o .bin/release --includeDirs ['src/**/*'] --build src/index.ts",
    "pack": "node ./scripts/build.js -o .bin/release --includeDirs ['src/**/*'] --build src/index.ts && node ./scripts/omitPackageJson.js --outDir .bin/release && npm pack .bin/release --pack-destination .bin/",
    "build-debug": "node ./scripts/build.js -o .bin/debug --includeDirs ['src/**/*','debug/**/*','.config/**/*'] --build debug/index.ts --sourceMap",
    "build-dry": "node ./scripts/build.js -o .bin/release --includeDirs ['src/**/*'] --build src/index.ts --dry-run",
    "lint": "node ./node_modules/eslint/bin/eslint.js src/**/*",
    "test-coverage": "jest --collectCoverage",
    "test": "jest",
    "clean": "node ./scripts/clean.js"
  },
  "keywords": [
    "pnp",
    "sharepoint",
    "react",
    "sp"
  ],
  "author": "Timur Olur",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-modules-commonjs": "^7.16.0",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@pnp/nodejs": "^2.9.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/args": "^3.0.1",
    "@types/jest": "^27.0.2",
    "@types/jsdom": "^16.2.13",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "args": "^5.0.1",
    "babel-jest": "^27.3.1",
    "colors": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.3.1",
    "jsdom": "^17.0.0",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-delete": "^2.0.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "rxjs": "^7.3.0",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "@pnp/sp": "^2.9.0",
    "react": ">=16.9.0 <18.0.0"
  }
}
