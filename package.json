{
  "type": "module",
  "engines": {
    "node": ">= 14.0.0"
  },
  "name": "pnp-react-hooks",
  "version": "0.0.0",
  "description": "PnPJS React hooks library.",
  "main": "index.js",
  "scripts": {
    "build": "node ./scripts/build.js -o ./.temp/bin/release --includeDirs ['src/**/*'] --build src/index.ts",
    "build-debug": "node ./scripts/build.js -o ./.temp/bin/debug --includeDirs ['src/**/*','debug/**/*','.config/**/*'] --build debug/index.ts --sourceMap",
    "build-dry": "node ./scripts/build.js -o ./.temp/bin/release --includeDirs ['src/**/*'] --build src/index.ts --dry-run",
    "pack": "node ./scripts/build.js -o ./.temp/bin/release --includeDirs ['src/**/*'] --build src/index.ts && node ./scripts/omitPackageJson.js --outDir .temp/bin/release && npm pack ./.temp/bin/release --pack-destination ./.temp/bin/",
    "clean": "node ./scripts/clean.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js src/**/*",
    "test": "jest"
  },
  "keywords": [
    "pnp",
    "sharepoint",
    "react",
    "sp"
  ],
  "author": "Timur Olur",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-modules-commonjs": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@pnp/nodejs": "^2.11.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/args": "^5.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.0.3",
    "@types/jsdom": "^16.2.14",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "args": "^5.0.1",
    "babel-jest": "^27.4.5",
    "eslint": "^8.5.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.4.5",
    "jsdom": "^19.0.0",
    "node-fetch": "^3.1.0",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.62.0",
    "rollup-plugin-delete": "^2.0.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "rxjs": "^7.5.1",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "@pnp/sp": "^2.11.0",
    "react": ">=16.9.0 <18.0.0"
  }
}
