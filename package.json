{
  "type": "module",
  "engines": {
    "node": ">= 14.0.0"
  },
  "name": "pnp-react-hooks",
  "version": "1.1.0",
  "description": "PnPJS React hooks library.",
  "main": "index.js",
  "scripts": {
    "build": "node ./scripts/multiBuild.js",
    "build-debug": "node ./scripts/multiBuild.js -c Debug",
    "pack-local": "node ./scripts/multiBuild.js -c Release && node ./scripts/omitPackageJson.js --outDir .temp/bin/release && npm pack ./.temp/bin/release --pack-destination ./.temp/bin/",
    "pack": "node ./scripts/multiBuild.js -c Release && node ./scripts/omitPackageJson.js --outDir .temp/bin/release",
    "clean": "node ./scripts/clean.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js src/**/*",
    "test": "jest"
  },
  "keywords": [
    "pnp",
    "sharepoint",
    "react",
    "sp",
    "hooks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SuperioOne/pnp-react-hooks"
  },
  "homepage": "https://superioone.github.io/pnp-react-hooks",
  "author": "Timur Olur",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pnp/nodejs": "^3.6.0",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/args": "^5.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.6",
    "@types/jsdom": "^20.0.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "args": "^5.0.3",
    "babel-jest": "^28.1.3",
    "eslint": "^8.20.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^10.1.0",
    "jest": "^28.1.3",
    "jsdom": "^20.0.0",
    "node-fetch": "^3.2.9",
    "react-dom": "^17.0.2",
    "rollup": "^2.77.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0"
  },
  "peerDependencies": {
    "@pnp/sp": "^3.6.0",
    "react": ">=16.9.0 <18.0.0"
  }
}
