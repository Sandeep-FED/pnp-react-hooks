{
  "type": "module",
  "engines": {
    "node": ">= 14.0.0"
  },
  "name": "pnp-react-hooks",
  "version": "1.0.0-rc.0",
  "description": "PnPJS React hooks library.",
  "main": "index.js",
  "scripts": {
    "build": "node ./scripts/multiBuild.js",
    "build-debug": "node ./scripts/multiBuild.js -c Debug",
    "pack-local": "node ./scripts/multiBuild.js -c Release && node ./scripts/omitPackageJson.js --outDir .temp/bin/release && npm pack ./.temp/bin/release --pack-destination ./.temp/bin/",
    "pack": "node ./scripts/multiBuild.js -c Release && node ./scripts/omitPackageJson.js --outDir .temp/bin/release",
    "clean": "node ./scripts/clean.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js src/**/*",
    "docs-api": "typedoc --plugin typedoc-plugin-markdown --out ./docs/temp src/hooks/sp/*.ts src/context/index.ts src/types/**/*.ts",
    "test": "jest"
  },
  "keywords": [
    "pnp",
    "sharepoint",
    "react",
    "sp",
    "hooks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pnp-react-hooks.git"
  },
  "author": "Timur Olur",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.3",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@pnp/nodejs": "^3.0.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/args": "^5.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^27.4.0",
    "@types/jsdom": "^16.2.14",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "args": "^5.0.1",
    "babel-jest": "^27.5.1",
    "eslint": "^8.9.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.5.1",
    "jsdom": "^19.0.0",
    "react-dom": "^17.0.2",
    "rollup": "^2.67.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.5.4",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.22.11",
    "typedoc-plugin-markdown": "^3.11.13",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "rxjs": "^7.5.4",
    "tslib": "^2.3.1"
  },
  "peerDependencies": {
    "@pnp/sp": "^3.0.3",
    "react": ">=16.9.0 <18.0.0"
  }
}
