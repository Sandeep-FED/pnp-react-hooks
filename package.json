{
  "type": "module",
  "engines": {
    "node": ">= 14.0.0"
  },
  "name": "pnp-react-hooks",
  "version": "1.3.0",
  "description": "PnPJS React hooks library.",
  "main": "index.js",
  "scripts": {
    "build": "node ./scripts/build.js -c Release",
    "build-debug": "node ./scripts/build.js -c Debug",
    "pack-local": "node ./scripts/build.js -c Release && node ./scripts/omitPackageJson.js --outDir .temp/bin/release && npm pack ./.temp/bin/release --pack-destination ./.temp/bin/",
    "pack": "node ./scripts/build.js -c Release && node ./scripts/omitPackageJson.js --outDir .temp/bin/release",
    "clean": "node ./scripts/clean.js",
    "lint": "node ./node_modules/eslint/bin/eslint.js src/**/*",
    "test": "jest"
  },
  "keywords": [
    "pnp",
    "sharepoint",
    "react",
    "sp",
    "hooks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SuperioOne/pnp-react-hooks"
  },
  "homepage": "https://superioone.github.io/pnp-react-hooks",
  "author": "Timur Olur",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/plugin-transform-modules-commonjs": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@pnp/nodejs": "^3.24.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/args": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^17.0.80",
    "@types/react-dom": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "args": "^5.0.3",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "jsdom": "^22.1.0",
    "node-fetch": "^3.3.2",
    "react-dom": "^17.0.2",
    "rollup": "^3.29.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@pnp/core": "^4.1.0",
    "@pnp/logging": "^4.1.0",
    "@pnp/queryable": "^4.1.0",
    "@pnp/sp": "^4.1.0",
    "react": ">=16.9.0 <18.0.0"
  }
}
